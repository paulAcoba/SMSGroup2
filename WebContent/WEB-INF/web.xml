<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>SMS_2</display-name>
  <welcome-file-list>
    <welcome-file>views/issuedsupply.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>issuedSupply</servlet-name>
  	<servlet-class>com.cpi.servlet.IssuesSupplyServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>issuedSupply</servlet-name>
  	<url-pattern>/issuedSupply</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>supplymaintenance</servlet-name>
  	<servlet-class>com.cpi.servlet.SuppliesMaintenanceServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
